{{#if generated_messages }}
<div class="table-responsive">
    <table id="products" class="table table-dark table-bordered table-striped">
        <tr>
            <th>email</th>
            <th>Timestamp</th>
            <th>Mensaje</th>
            <th>Avatar</th>
        </tr>
        {{#each generated_messages}}
        <tr>
            <td>
                {{this.author.email}}
            </td>
            <td>
                {{this.timestamp}}
            </td>
            <td>
                {{this.text}}
            </td>
            <td>
                <img src={{this.author.avatar}}>
            </td>
        </tr>
        {{/each}}
    </table>
</div>
{{else}}
<h3 id="noHay" class="bg-light text-danger">No hay mensajes</h3>
{{/if}}