{{>_buttonGroup}}

<body>
    <div class="container mt-3">
        <div class="jumbotron">
            <h1 class="text-primary">Ingrese producto</h1>
            <br>

            <form id="productForm" action="">

                <div class="form-group">
                    <label for="title"><b>Nombre</b></label>
                    <input id="title" class="form-control" type="text" name="title">
                </div>

                <div class="form-group">
                    <label for="price"><b>Precio</b></label>
                    <input id="price" class="form-control" type="number" name="price">
                </div>

                <div class="form-group">
                    <label for="thumbnail"><b>Imagen</b></label>
                    <input id="thumbnail" class="form-control" type="text" name="thumbnail">
                </div>

                <button type="submit" class="btn-success">Enviar</button>
            </form>

        </div>
        {{>_table}}
    </div>
    <div class="bg-light m-5" width="80%">
        <h1 class="text-primary">Centro de mensajes</h1>
        <ul id="messages"></ul>
        <form id="form" action="">
            <div width="auto">
                <div>
                    <input class="input" type="email" id="username" value="e@w" required />
                    <input class="input" type="text" id="name" autocomplete="off" placeholder="Nombre " />
                    <input class="input" type="text" id="last_name" value="Apellido" required />
                <div>
                    <input class="input" type="number" id="age" autocomplete="off" placeholder="Edad " />
                    <input class="input" type="text" id="nickname" value="Alias" required />
                    <input class="input" type="text" id="avatar" autocomplete="off" placeholder="Imagen" />
                </div>
                <div>
                    <input class="inputl" type="text" id="message" autocomplete="off" placeholder="Mensaje" />
                </div>
                <div>
                    <button class="btn-primary" type="submit">Enviar</button>
                </div>
            </div>
        </form>
        <script src="../index.js"></script>
    </div>
</body>